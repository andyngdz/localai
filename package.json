{
  "name": "localai",
  "description": "LocalAI",
  "version": "1.15.0",
  "private": true,
  "packageManager": "pnpm@10.23.0",
  "author": {
    "name": "Andy D. Nguyen",
    "email": "info@andyng.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andyngdz/localai.git"
  },
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev --turbo",
    "desktop": "tsx scripts/devall.ts",
    "desktop:local": "cross-env SKIP_BACKEND=true tsx scripts/devall.ts",
    "compile": "tsx scripts/compile.ts",
    "build": "pnpm run compile && next build --turbo && electron-builder",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "ci:lint": "eslint src --ext .ts,.tsx,.js,.jsx --max-warnings=0 -f json -o eslint-report.json",
    "ci:format": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "test": "vitest --run",
    "test:coverage": "vitest --coverage",
    "semantic-release": "semantic-release",
    "prepare": "husky",
    "fix:sandbox": "sudo sysctl -w kernel.apparmor_restrict_unprivileged_userns=0"
  },
  "dependencies": {
    "@heroui/react": "^2.8.5",
    "@heroui/system": "^2.4.23",
    "@heroui/theme": "^2.4.23",
    "@heroui/use-disclosure": "^2.2.17",
    "@react-aria/interactions": "^3.25.6",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-virtual": "^3.13.12",
    "@uidotdev/usehooks": "^2.4.1",
    "allotment": "^1.20.4",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "core-js": "^3.47.0",
    "dayjs": "^1.11.19",
    "electron-log": "^5.4.3",
    "electron-serve": "^3.0.0",
    "electron-updater": "^6.6.2",
    "es-toolkit": "^1.42.0",
    "fix-path": "^5.0.0",
    "framer-motion": "^12.23.24",
    "immer": "^10.2.0",
    "lucide-react": "^0.547.0",
    "next": "^16.0.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.1",
    "react-lottie": "^1.2.10",
    "react-use": "^17.6.0",
    "socket.io-client": "^4.8.1",
    "strip-ansi": "^7.1.2",
    "swiper": "^12.0.3",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.1.17",
    "zustand": "^5.0.8",
    "zx": "^8.8.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/react-query-devtools": "^5.91.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/react-lottie": "^1.2.10",
    "@types/wait-on": "^5.3.4",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/spy": "^4.0.13",
    "concurrently": "^9.2.1",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "cross-env": "^10.1.0",
    "electron": "^38.7.1",
    "electron-builder": "^26.0.12",
    "esbuild": "^0.25.12",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "semantic-release": "^25.0.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.13",
    "vitest-localstorage-mock": "^0.1.2",
    "vitest-sonar-reporter": "^3.0.0",
    "wait-on": "^9.0.3"
  }
}
